{
  "name": "@prompt-gen/marketplace",
  "version": "0.1.0",
  "description": "Package registry and discovery platform for Prompt Gen ecosystem",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "dev": "tsx watch src/index.ts",
    "dev:frontend": "vite",
    "dev:full": "concurrently \"npm run dev\" \"npm run dev:frontend\"",
    "build": "tsc",
    "build:frontend": "vite build",
    "build:all": "npm run build && npm run build:frontend",
    "start": "node dist/index.js",
    "start:with-migrations": "npm run migrate:up && node scripts/populate-content-counts.cjs && npm run db:seed && npm run start",
    "test": "vitest",
    "test:coverage": "vitest --coverage",
    "test:integration": "vitest --config vitest.integration.config.ts",
    "migrate:create": "node-pg-migrate create -f .pgmigrate -m database/pgmigrations --migration-filename-format utc",
    "migrate:up": "node scripts/run-migrations.cjs up",
    "migrate:down": "node scripts/run-migrations.cjs down",
    "migrate:latest": "npm run migrate:up",
    "migrate:reset": "node scripts/run-migrations.cjs reset",
    "db:seed": "tsx database/seed-oauth-clients.ts",
    "lint": "eslint src --ext .ts --max-warnings 200",
    "lint:fix": "eslint src --ext .ts --fix",
    "lint:frontend": "eslint frontend/src --ext .ts,.vue --max-warnings 33",
    "lint:frontend:fix": "eslint frontend/src --ext .ts,.vue --fix",
    "format": "prettier --write \"src/**/*.ts\" \"frontend/src/**/*.{ts,vue}\"",
    "format:check": "prettier --check \"src/**/*.ts\" \"frontend/src/**/*.{ts,vue}\"",
    "type-check": "tsc --noEmit",
    "type-check:frontend": "vue-tsc --noEmit -p frontend/tsconfig.json",
    "validate": "npm run format:check && npm run lint && npm run lint:frontend && npm run type-check && npm run type-check:frontend && npm run test -- --run && npm run build && npm run build:frontend",
    "validate:ci": "powershell -NoProfile -ExecutionPolicy Bypass -File scripts/ci-local.ps1",
    "validate:integration": "npm run test:integration -- --run",
    "db:init": "node -e \"console.error('Deprecated: use npm run migrate:up'); process.exit(1)\"",
    "db:init:railway": "node -e \"console.error('Deprecated: use npm run migrate:up'); process.exit(1)\""
  },
  "keywords": [
    "prompt-generation",
    "marketplace",
    "package-registry",
    "oauth"
  ],
  "author": "Prompt Gen Team",
  "license": "MIT",
  "dependencies": {
    "@noble/ed25519": "^3.0.0",
    "@noble/hashes": "^2.0.1",
    "aws-sdk": "^2.1506.0",
    "bcrypt": "^5.1.1",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-async-handler": "^1.2.0",
    "express-rate-limit": "^7.1.5",
    "express-validator": "^7.0.1",
    "helmet": "^7.1.0",
    "jose": "^5.1.3",
    "js-yaml": "^4.1.0",
    "jsonwebtoken": "^9.0.2",
    "nanoid": "^5.0.4",
    "pg": "^8.11.3",
    "redis": "^4.6.11",
    "semver": "^7.5.4",
    "tweetnacl": "^1.0.3",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.2",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/js-yaml": "^4.0.9",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/node": "^20.10.6",
    "@types/semver": "^7.5.6",
    "@types/supertest": "^6.0.2",
    "@types/uuid": "^10.0.0",
    "@typescript-eslint/eslint-plugin": "^6.17.0",
    "@typescript-eslint/parser": "^6.17.0",
    "@vitejs/plugin-vue": "^6.0.3",
    "@vitest/coverage-v8": "^1.1.0",
    "@vue/eslint-config-typescript": "^13.0.0",
    "concurrently": "^9.2.1",
    "cross-env": "^7.0.3",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.4",
    "eslint-plugin-vue": "^9.20.1",
    "node-pg-migrate": "^6.2.2",
    "prettier": "^3.1.1",
    "supertest": "^6.3.3",
    "tsx": "^4.7.0",
    "typescript": "^5.3.3",
    "vite": "^7.3.0",
    "vitest": "^1.1.0",
    "vue": "^3.5.26",
    "vue-router": "^4.6.4",
    "vue-tsc": "^2.0.0"
  },
  "engines": {
    "node": ">=20.0.0"
  }
}
